package com.springbackend.springboot.service;

import java.util.List;

import org.springframework.http.ResponseEntity;
import org.springframework.stereotype.Service;


import com.springbackend.springboot.model.Doctor;
import com.springbackend.springboot.model.Patient;

@Service
public interface DoctorPatientService {
	
	 public List<Doctor> getAllEmployees1();
	 public Doctor createEmployee();
	 public ResponseEntity<Doctor> getEmployeeById1(Long id);
	 public List<Patient> getAllEmployees();
	 public Patient createEmployeee(Patient patient);
	 public ResponseEntity<Patient> getEmployeeById(Long id);

}
